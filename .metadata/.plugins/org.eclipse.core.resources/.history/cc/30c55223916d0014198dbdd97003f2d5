package ndnex.dsu.androidrepo.db;

public class AppFilterTlv {

	/*
	 * TLV Design:
	 * Filter := FILTER-TYPE TLV-LENGTH
	 * 				Bound
	 * 				Precision
	 * 				Frequency?
	 * 
	 * Bound := BOUND-TYPE TLV-LENGTH
	 * 				BoundTime?
	 * 				BoundLocation
	 * 				BoundNumber
	 * 				Text
	 * 
	 * Precision:= PRECISION-TYPE TLV-LENGTH
	 * 				PrecisionTime
	 * 				PrecisionLocation
	 * 				PrecisionNumber
	 * 
	 * Frequency := FREQUENCY-TYPE TLV-LENGTH
	 * 				FrequencyTime
	 * 
	 * BoundTime := BOUNDTIME-TYPE TLV-LENGTH
	 * 				StartTime
	 * 				EndTime
	 * 
	 * StartTime := STARTTIME-TYPE TLV-LENGTH
	 * 				NonNegativeInteger
	 * 
	 * EndTime := END
	 * 
	 * BoundLocation := 
	 */
}
